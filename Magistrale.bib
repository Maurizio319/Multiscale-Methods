@article{huSARSCoV2MainProtease2022,
  title = {The {{SARS-CoV-2}} Main Protease ({{Mpro}}): {{Structure}}, Function, and Emerging Therapies for {{COVID-19}}},
  shorttitle = {The {{SARS-CoV-2}} Main Protease ({{Mpro}})},
  author = {Hu, Qing and Xiong, Yuan and Zhu, Guang-Hao and Zhang, Ya-Ni and Zhang, Yi-Wen and Huang, Ping and Ge, Guang-Bo},
  date = {2022},
  journaltitle = {MedComm},
  volume = {3},
  number = {3},
