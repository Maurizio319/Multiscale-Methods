@article{knellerUnusualZwitterionicCatalytic2020,
  title = {Unusual Zwitterionic Catalytic Site of {{SARS-CoV-2}} Main Protease Revealed by Neutron Crystallography},
  author = {Kneller, Daniel W. and Phillips, Gwyndalyn and Weiss, Kevin L. and Pant, Swati and Zhang, Qiu and O'Neill, Hugh M. and Coates, Leighton and Kovalevsky, Andrey},
