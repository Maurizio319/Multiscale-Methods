  author = {Rostkowski, Michał and Olsson, Mats HM and Søndergaard, Chresten R and Jensen, Jan H},
  date = {2011-01-26},
  journaltitle = {BMC Structural Biology},
  shortjournal = {BMC Struct Biol},
  volume = {11},
  eprint = {21269479},
