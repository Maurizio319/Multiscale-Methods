@article{munroStructureFunctionCytochrome2018,
  title = {Structure and Function of the Cytochrome {{P450}} Peroxygenase Enzymes},
  author = {Munro, Andrew W. and McLean, Kirsty J. and Grant, Job L. and Makris, Thomas M.},
  date = {2018-02-06},
  journaltitle = {Biochemical Society Transactions},
  shortjournal = {Biochemical Society Transactions},
  volume = {46},
  number = {1},
  pages = {183--196},
