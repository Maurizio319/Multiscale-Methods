@article{yangDesignWideSpectrumInhibitors2005,
  title = {Design of {{Wide-Spectrum Inhibitors Targeting Coronavirus Main Proteases}}},
  author = {Yang, Haitao and Xie, Weiqing and Xue, Xiaoyu and Yang, Kailin and Ma, Jing and Liang, Wenxue and Zhao, Qi and Zhou, Zhe and Pei, Duanqing and Ziebuhr, John and Hilgenfeld, Rolf and Yuen, Kwok Yung and Wong, Luet and Gao, Guangxia and Chen, Saijuan and Chen, Zhu and Ma, Dawei and Bartlam, Mark and Rao, Zihe},
  date = {2005-09-06},
  journaltitle = {PLOS Biology},
