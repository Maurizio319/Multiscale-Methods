  author = {Moser, Felix and Broers, Nicolette J. and Hartmans, Sybe and Tamsir, Alvin and Kerkman, Richard and Roubos, Johannes A. and Bovenberg, Roel and Voigt, Christopher A.},
  date = {2012-11-16},
  journaltitle = {ACS Synthetic Biology},
  shortjournal = {ACS Synth. Biol.},
