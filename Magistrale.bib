  shorttitle = {{{MEME SUITE}}},
  author = {Bailey, Timothy L. and Boden, Mikael and Buske, Fabian A. and Frith, Martin and Grant, Charles E. and Clementi, Luca and Ren, Jingyuan and Li, Wilfred W. and Noble, William S.},
